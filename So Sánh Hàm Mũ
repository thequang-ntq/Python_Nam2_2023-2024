a,b,c = map(int,input().split())
if a > b:
    if c % 2 == 0:
        if(abs(a) > abs(b)):
            print(">")
        else:
            print("<")
    else: 
        print(">")
elif a == b:
    print("=")
else:
    if c % 2 == 0:
        if(abs(a) == abs(b)):
            print("=")
        else:
            if(abs(a) > abs(b)):
                print(">")
            else:
                print("<")
    else:
        print("<")
